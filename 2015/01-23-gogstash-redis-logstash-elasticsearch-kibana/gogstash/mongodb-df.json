{
	"input": [
		{
			"type": "exec",
			"command": "bash",
			"interval": 60,
			"args": ["-c", "date -Iseconds ; df -B 1 /"]
		}
	],
	"output": [
		{
			"type": "report"
		},
		{
			"type": "redis",
			"key": "log-mongodb-df",
			"host": ["redis-1.localnet.local:6379", "redis-2.localnet.local:6379", "redis-3.localnet.local:6379"]
		}
	]
}
